{
  "name": "yousef-recharge-api",
  "version": "1.0.0",
  "description": "Yousef Recharge Fitness Platform API",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run db:generate",
    "build:client": "vite build",
    "start": "tsx server/index.ts",
    "start:api": "node dist/server/index.js",
    "db:generate": "prisma generate",
    "db:deploy": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "postinstall": "npm run db:generate",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc"
  },
